generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Url {
  id        String   @id @default(uuid())
  short     String   @unique
  full      String
  authorId  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
